<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_cmspPayInfo"  extends="struts_common">
	  
	  	<!-- CmspPayInfo列表信息 -->
        <action name="searchCmspPayInfoAction" class="searchCmspPayInfoAction">
			<result name="success" >/jsp/demo/searchCmspPayInfoPage.jsp</result>
		</action>
		
        <!-- CmspPayInfo新增页面-->
        <action name="addCmspPayInfoPage" class="addCmspPayInfoPage">
			<result name="success" >/jsp/demo/addCmspPayInfoPage.jsp</result>
		</action>
		
		<!-- CmspPayInfo新增动作	-->
        <action name="addCmspPayInfoAction" class="addCmspPayInfoAction"> 
			<result name="success" type="chain"><param name="actionName">searchCmspPayInfoAction</param> </result>
		</action>
		
		<!-- CmspPayInfo编辑页面 -->
		<action name="editCmspPayInfoPage" class="editCmspPayInfoPage">
			<result name="success" >/jsp/demo/editCmspPayInfoPage.jsp</result>
		</action>

		
		<!-- CmspPayInfo编辑动作-->
		<action name="editCmspPayInfoAction" class="editCmspPayInfoAction">
			<result name="success" type="chain"><param name="actionName">searchCmspPayInfoAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailCmspPayInfoPage" class="detailCmspPayInfoPage">
			<result name="success" >/jsp/demo/detailCmspPayInfoPage.jsp</result>
		</action>
		
		
		<!-- CmspPayInfo删除页面	-->
		<action name="delCmspPayInfoAction" class="delCmspPayInfoAction">
			<result name="success" type="chain"><param name="actionName">searchCmspPayInfoAction</param> </result>
		</action>		

      
	</package>
</struts>