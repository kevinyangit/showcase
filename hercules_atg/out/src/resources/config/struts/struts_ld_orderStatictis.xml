<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_orderStatictis"  extends="struts_common">
	  
	  	<!-- OrderStatictis列表信息 -->
        <action name="searchOrderStatictisAction" class="searchOrderStatictisAction">
			<result name="success" >/jsp/demo/searchOrderStatictisPage.jsp</result>
		</action>
		
        <!-- OrderStatictis新增页面-->
        <action name="addOrderStatictisPage" class="addOrderStatictisPage">
			<result name="success" >/jsp/demo/addOrderStatictisPage.jsp</result>
		</action>
		
		<!-- OrderStatictis新增动作	-->
        <action name="addOrderStatictisAction" class="addOrderStatictisAction"> 
			<result name="success" type="chain"><param name="actionName">searchOrderStatictisAction</param> </result>
		</action>
		
		<!-- OrderStatictis编辑页面 -->
		<action name="editOrderStatictisPage" class="editOrderStatictisPage">
			<result name="success" >/jsp/demo/editOrderStatictisPage.jsp</result>
		</action>

		
		<!-- OrderStatictis编辑动作-->
		<action name="editOrderStatictisAction" class="editOrderStatictisAction">
			<result name="success" type="chain"><param name="actionName">searchOrderStatictisAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailOrderStatictisPage" class="detailOrderStatictisPage">
			<result name="success" >/jsp/demo/detailOrderStatictisPage.jsp</result>
		</action>
		
		
		<!-- OrderStatictis删除页面	-->
		<action name="delOrderStatictisAction" class="delOrderStatictisAction">
			<result name="success" type="chain"><param name="actionName">searchOrderStatictisAction</param> </result>
		</action>		

      
	</package>
</struts>