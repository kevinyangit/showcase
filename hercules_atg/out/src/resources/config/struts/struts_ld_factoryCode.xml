<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_factoryCode"  extends="struts_common">
	  
	  	<!-- FactoryCode列表信息 -->
        <action name="searchFactoryCodeAction" class="searchFactoryCodeAction">
			<result name="success" >/jsp/demo/searchFactoryCodePage.jsp</result>
		</action>
		
        <!-- FactoryCode新增页面-->
        <action name="addFactoryCodePage" class="addFactoryCodePage">
			<result name="success" >/jsp/demo/addFactoryCodePage.jsp</result>
		</action>
		
		<!-- FactoryCode新增动作	-->
        <action name="addFactoryCodeAction" class="addFactoryCodeAction"> 
			<result name="success" type="chain"><param name="actionName">searchFactoryCodeAction</param> </result>
		</action>
		
		<!-- FactoryCode编辑页面 -->
		<action name="editFactoryCodePage" class="editFactoryCodePage">
			<result name="success" >/jsp/demo/editFactoryCodePage.jsp</result>
		</action>

		
		<!-- FactoryCode编辑动作-->
		<action name="editFactoryCodeAction" class="editFactoryCodeAction">
			<result name="success" type="chain"><param name="actionName">searchFactoryCodeAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailFactoryCodePage" class="detailFactoryCodePage">
			<result name="success" >/jsp/demo/detailFactoryCodePage.jsp</result>
		</action>
		
		
		<!-- FactoryCode删除页面	-->
		<action name="delFactoryCodeAction" class="delFactoryCodeAction">
			<result name="success" type="chain"><param name="actionName">searchFactoryCodeAction</param> </result>
		</action>		

      
	</package>
</struts>