<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_orderGoods"  extends="struts_common">
	  
	  	<!-- OrderGoods列表信息 -->
        <action name="searchOrderGoodsAction" class="searchOrderGoodsAction">
			<result name="success" >/jsp/demo/searchOrderGoodsPage.jsp</result>
		</action>
		
        <!-- OrderGoods新增页面-->
        <action name="addOrderGoodsPage" class="addOrderGoodsPage">
			<result name="success" >/jsp/demo/addOrderGoodsPage.jsp</result>
		</action>
		
		<!-- OrderGoods新增动作	-->
        <action name="addOrderGoodsAction" class="addOrderGoodsAction"> 
			<result name="success" type="chain"><param name="actionName">searchOrderGoodsAction</param> </result>
		</action>
		
		<!-- OrderGoods编辑页面 -->
		<action name="editOrderGoodsPage" class="editOrderGoodsPage">
			<result name="success" >/jsp/demo/editOrderGoodsPage.jsp</result>
		</action>

		
		<!-- OrderGoods编辑动作-->
		<action name="editOrderGoodsAction" class="editOrderGoodsAction">
			<result name="success" type="chain"><param name="actionName">searchOrderGoodsAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailOrderGoodsPage" class="detailOrderGoodsPage">
			<result name="success" >/jsp/demo/detailOrderGoodsPage.jsp</result>
		</action>
		
		
		<!-- OrderGoods删除页面	-->
		<action name="delOrderGoodsAction" class="delOrderGoodsAction">
			<result name="success" type="chain"><param name="actionName">searchOrderGoodsAction</param> </result>
		</action>		

      
	</package>
</struts>