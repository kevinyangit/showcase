<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_errOrderInfo"  extends="struts_common">
	  
	  	<!-- ErrOrderInfo列表信息 -->
        <action name="searchErrOrderInfoAction" class="searchErrOrderInfoAction">
			<result name="success" >/jsp/demo/searchErrOrderInfoPage.jsp</result>
		</action>
		
        <!-- ErrOrderInfo新增页面-->
        <action name="addErrOrderInfoPage" class="addErrOrderInfoPage">
			<result name="success" >/jsp/demo/addErrOrderInfoPage.jsp</result>
		</action>
		
		<!-- ErrOrderInfo新增动作	-->
        <action name="addErrOrderInfoAction" class="addErrOrderInfoAction"> 
			<result name="success" type="chain"><param name="actionName">searchErrOrderInfoAction</param> </result>
		</action>
		
		<!-- ErrOrderInfo编辑页面 -->
		<action name="editErrOrderInfoPage" class="editErrOrderInfoPage">
			<result name="success" >/jsp/demo/editErrOrderInfoPage.jsp</result>
		</action>

		
		<!-- ErrOrderInfo编辑动作-->
		<action name="editErrOrderInfoAction" class="editErrOrderInfoAction">
			<result name="success" type="chain"><param name="actionName">searchErrOrderInfoAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailErrOrderInfoPage" class="detailErrOrderInfoPage">
			<result name="success" >/jsp/demo/detailErrOrderInfoPage.jsp</result>
		</action>
		
		
		<!-- ErrOrderInfo删除页面	-->
		<action name="delErrOrderInfoAction" class="delErrOrderInfoAction">
			<result name="success" type="chain"><param name="actionName">searchErrOrderInfoAction</param> </result>
		</action>		

      
	</package>
</struts>