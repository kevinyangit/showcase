<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_merOpLog"  extends="struts_common">
	  
	  	<!-- MerOpLog列表信息 -->
        <action name="searchMerOpLogAction" class="searchMerOpLogAction">
			<result name="success" >/jsp/demo/searchMerOpLogPage.jsp</result>
		</action>
		
        <!-- MerOpLog新增页面-->
        <action name="addMerOpLogPage" class="addMerOpLogPage">
			<result name="success" >/jsp/demo/addMerOpLogPage.jsp</result>
		</action>
		
		<!-- MerOpLog新增动作	-->
        <action name="addMerOpLogAction" class="addMerOpLogAction"> 
			<result name="success" type="chain"><param name="actionName">searchMerOpLogAction</param> </result>
		</action>
		
		<!-- MerOpLog编辑页面 -->
		<action name="editMerOpLogPage" class="editMerOpLogPage">
			<result name="success" >/jsp/demo/editMerOpLogPage.jsp</result>
		</action>

		
		<!-- MerOpLog编辑动作-->
		<action name="editMerOpLogAction" class="editMerOpLogAction">
			<result name="success" type="chain"><param name="actionName">searchMerOpLogAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailMerOpLogPage" class="detailMerOpLogPage">
			<result name="success" >/jsp/demo/detailMerOpLogPage.jsp</result>
		</action>
		
		
		<!-- MerOpLog删除页面	-->
		<action name="delMerOpLogAction" class="delMerOpLogAction">
			<result name="success" type="chain"><param name="actionName">searchMerOpLogAction</param> </result>
		</action>		

      
	</package>
</struts>