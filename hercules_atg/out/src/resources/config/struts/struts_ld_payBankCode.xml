<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_payBankCode"  extends="struts_common">
	  
	  	<!-- PayBankCode列表信息 -->
        <action name="searchPayBankCodeAction" class="searchPayBankCodeAction">
			<result name="success" >/jsp/demo/searchPayBankCodePage.jsp</result>
		</action>
		
        <!-- PayBankCode新增页面-->
        <action name="addPayBankCodePage" class="addPayBankCodePage">
			<result name="success" >/jsp/demo/addPayBankCodePage.jsp</result>
		</action>
		
		<!-- PayBankCode新增动作	-->
        <action name="addPayBankCodeAction" class="addPayBankCodeAction"> 
			<result name="success" type="chain"><param name="actionName">searchPayBankCodeAction</param> </result>
		</action>
		
		<!-- PayBankCode编辑页面 -->
		<action name="editPayBankCodePage" class="editPayBankCodePage">
			<result name="success" >/jsp/demo/editPayBankCodePage.jsp</result>
		</action>

		
		<!-- PayBankCode编辑动作-->
		<action name="editPayBankCodeAction" class="editPayBankCodeAction">
			<result name="success" type="chain"><param name="actionName">searchPayBankCodeAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailPayBankCodePage" class="detailPayBankCodePage">
			<result name="success" >/jsp/demo/detailPayBankCodePage.jsp</result>
		</action>
		
		
		<!-- PayBankCode删除页面	-->
		<action name="delPayBankCodeAction" class="delPayBankCodeAction">
			<result name="success" type="chain"><param name="actionName">searchPayBankCodeAction</param> </result>
		</action>		

      
	</package>
</struts>