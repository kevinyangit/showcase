<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_rulesMerAmt"  extends="struts_common">
	  
	  	<!-- RulesMerAmt列表信息 -->
        <action name="searchRulesMerAmtAction" class="searchRulesMerAmtAction">
			<result name="success" >/jsp/demo/searchRulesMerAmtPage.jsp</result>
		</action>
		
        <!-- RulesMerAmt新增页面-->
        <action name="addRulesMerAmtPage" class="addRulesMerAmtPage">
			<result name="success" >/jsp/demo/addRulesMerAmtPage.jsp</result>
		</action>
		
		<!-- RulesMerAmt新增动作	-->
        <action name="addRulesMerAmtAction" class="addRulesMerAmtAction"> 
			<result name="success" type="chain"><param name="actionName">searchRulesMerAmtAction</param> </result>
		</action>
		
		<!-- RulesMerAmt编辑页面 -->
		<action name="editRulesMerAmtPage" class="editRulesMerAmtPage">
			<result name="success" >/jsp/demo/editRulesMerAmtPage.jsp</result>
		</action>

		
		<!-- RulesMerAmt编辑动作-->
		<action name="editRulesMerAmtAction" class="editRulesMerAmtAction">
			<result name="success" type="chain"><param name="actionName">searchRulesMerAmtAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailRulesMerAmtPage" class="detailRulesMerAmtPage">
			<result name="success" >/jsp/demo/detailRulesMerAmtPage.jsp</result>
		</action>
		
		
		<!-- RulesMerAmt删除页面	-->
		<action name="delRulesMerAmtAction" class="delRulesMerAmtAction">
			<result name="success" type="chain"><param name="actionName">searchRulesMerAmtAction</param> </result>
		</action>		

      
	</package>
</struts>