<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_orderProfit"  extends="struts_common">
	  
	  	<!-- OrderProfit列表信息 -->
        <action name="searchOrderProfitAction" class="searchOrderProfitAction">
			<result name="success" >/jsp/demo/searchOrderProfitPage.jsp</result>
		</action>
		
        <!-- OrderProfit新增页面-->
        <action name="addOrderProfitPage" class="addOrderProfitPage">
			<result name="success" >/jsp/demo/addOrderProfitPage.jsp</result>
		</action>
		
		<!-- OrderProfit新增动作	-->
        <action name="addOrderProfitAction" class="addOrderProfitAction"> 
			<result name="success" type="chain"><param name="actionName">searchOrderProfitAction</param> </result>
		</action>
		
		<!-- OrderProfit编辑页面 -->
		<action name="editOrderProfitPage" class="editOrderProfitPage">
			<result name="success" >/jsp/demo/editOrderProfitPage.jsp</result>
		</action>

		
		<!-- OrderProfit编辑动作-->
		<action name="editOrderProfitAction" class="editOrderProfitAction">
			<result name="success" type="chain"><param name="actionName">searchOrderProfitAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailOrderProfitPage" class="detailOrderProfitPage">
			<result name="success" >/jsp/demo/detailOrderProfitPage.jsp</result>
		</action>
		
		
		<!-- OrderProfit删除页面	-->
		<action name="delOrderProfitAction" class="delOrderProfitAction">
			<result name="success" type="chain"><param name="actionName">searchOrderProfitAction</param> </result>
		</action>		

      
	</package>
</struts>