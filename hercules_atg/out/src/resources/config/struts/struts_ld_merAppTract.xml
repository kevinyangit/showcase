<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_merAppTract"  extends="struts_common">
	  
	  	<!-- MerAppTract列表信息 -->
        <action name="searchMerAppTractAction" class="searchMerAppTractAction">
			<result name="success" >/jsp/demo/searchMerAppTractPage.jsp</result>
		</action>
		
        <!-- MerAppTract新增页面-->
        <action name="addMerAppTractPage" class="addMerAppTractPage">
			<result name="success" >/jsp/demo/addMerAppTractPage.jsp</result>
		</action>
		
		<!-- MerAppTract新增动作	-->
        <action name="addMerAppTractAction" class="addMerAppTractAction"> 
			<result name="success" type="chain"><param name="actionName">searchMerAppTractAction</param> </result>
		</action>
		
		<!-- MerAppTract编辑页面 -->
		<action name="editMerAppTractPage" class="editMerAppTractPage">
			<result name="success" >/jsp/demo/editMerAppTractPage.jsp</result>
		</action>

		
		<!-- MerAppTract编辑动作-->
		<action name="editMerAppTractAction" class="editMerAppTractAction">
			<result name="success" type="chain"><param name="actionName">searchMerAppTractAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailMerAppTractPage" class="detailMerAppTractPage">
			<result name="success" >/jsp/demo/detailMerAppTractPage.jsp</result>
		</action>
		
		
		<!-- MerAppTract删除页面	-->
		<action name="delMerAppTractAction" class="delMerAppTractAction">
			<result name="success" type="chain"><param name="actionName">searchMerAppTractAction</param> </result>
		</action>		

      
	</package>
</struts>