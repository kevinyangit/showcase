<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_cstpTerminal"  extends="struts_common">
	  
	  	<!-- CstpTerminal列表信息 -->
        <action name="searchCstpTerminalAction" class="searchCstpTerminalAction">
			<result name="success" >/jsp/demo/searchCstpTerminalPage.jsp</result>
		</action>
		
        <!-- CstpTerminal新增页面-->
        <action name="addCstpTerminalPage" class="addCstpTerminalPage">
			<result name="success" >/jsp/demo/addCstpTerminalPage.jsp</result>
		</action>
		
		<!-- CstpTerminal新增动作	-->
        <action name="addCstpTerminalAction" class="addCstpTerminalAction"> 
			<result name="success" type="chain"><param name="actionName">searchCstpTerminalAction</param> </result>
		</action>
		
		<!-- CstpTerminal编辑页面 -->
		<action name="editCstpTerminalPage" class="editCstpTerminalPage">
			<result name="success" >/jsp/demo/editCstpTerminalPage.jsp</result>
		</action>

		
		<!-- CstpTerminal编辑动作-->
		<action name="editCstpTerminalAction" class="editCstpTerminalAction">
			<result name="success" type="chain"><param name="actionName">searchCstpTerminalAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailCstpTerminalPage" class="detailCstpTerminalPage">
			<result name="success" >/jsp/demo/detailCstpTerminalPage.jsp</result>
		</action>
		
		
		<!-- CstpTerminal删除页面	-->
		<action name="delCstpTerminalAction" class="delCstpTerminalAction">
			<result name="success" type="chain"><param name="actionName">searchCstpTerminalAction</param> </result>
		</action>		

      
	</package>
</struts>