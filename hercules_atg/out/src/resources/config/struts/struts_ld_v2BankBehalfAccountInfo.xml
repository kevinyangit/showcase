<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_v2BankBehalfAccountInfo"  extends="struts_common">
	  
	  	<!-- V2BankBehalfAccountInfo列表信息 -->
        <action name="searchV2BankBehalfAccountInfoAction" class="searchV2BankBehalfAccountInfoAction">
			<result name="success" >/jsp/demo/searchV2BankBehalfAccountInfoPage.jsp</result>
		</action>
		
        <!-- V2BankBehalfAccountInfo新增页面-->
        <action name="addV2BankBehalfAccountInfoPage" class="addV2BankBehalfAccountInfoPage">
			<result name="success" >/jsp/demo/addV2BankBehalfAccountInfoPage.jsp</result>
		</action>
		
		<!-- V2BankBehalfAccountInfo新增动作	-->
        <action name="addV2BankBehalfAccountInfoAction" class="addV2BankBehalfAccountInfoAction"> 
			<result name="success" type="chain"><param name="actionName">searchV2BankBehalfAccountInfoAction</param> </result>
		</action>
		
		<!-- V2BankBehalfAccountInfo编辑页面 -->
		<action name="editV2BankBehalfAccountInfoPage" class="editV2BankBehalfAccountInfoPage">
			<result name="success" >/jsp/demo/editV2BankBehalfAccountInfoPage.jsp</result>
		</action>

		
		<!-- V2BankBehalfAccountInfo编辑动作-->
		<action name="editV2BankBehalfAccountInfoAction" class="editV2BankBehalfAccountInfoAction">
			<result name="success" type="chain"><param name="actionName">searchV2BankBehalfAccountInfoAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailV2BankBehalfAccountInfoPage" class="detailV2BankBehalfAccountInfoPage">
			<result name="success" >/jsp/demo/detailV2BankBehalfAccountInfoPage.jsp</result>
		</action>
		
		
		<!-- V2BankBehalfAccountInfo删除页面	-->
		<action name="delV2BankBehalfAccountInfoAction" class="delV2BankBehalfAccountInfoAction">
			<result name="success" type="chain"><param name="actionName">searchV2BankBehalfAccountInfoAction</param> </result>
		</action>		

      
	</package>
</struts>