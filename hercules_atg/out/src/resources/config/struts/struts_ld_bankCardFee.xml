<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_bankCardFee"  extends="struts_common">
	  
	  	<!-- BankCardFee列表信息 -->
        <action name="searchBankCardFeeAction" class="searchBankCardFeeAction">
			<result name="success" >/jsp/demo/searchBankCardFeePage.jsp</result>
		</action>
		
        <!-- BankCardFee新增页面-->
        <action name="addBankCardFeePage" class="addBankCardFeePage">
			<result name="success" >/jsp/demo/addBankCardFeePage.jsp</result>
		</action>
		
		<!-- BankCardFee新增动作	-->
        <action name="addBankCardFeeAction" class="addBankCardFeeAction"> 
			<result name="success" type="chain"><param name="actionName">searchBankCardFeeAction</param> </result>
		</action>
		
		<!-- BankCardFee编辑页面 -->
		<action name="editBankCardFeePage" class="editBankCardFeePage">
			<result name="success" >/jsp/demo/editBankCardFeePage.jsp</result>
		</action>

		
		<!-- BankCardFee编辑动作-->
		<action name="editBankCardFeeAction" class="editBankCardFeeAction">
			<result name="success" type="chain"><param name="actionName">searchBankCardFeeAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailBankCardFeePage" class="detailBankCardFeePage">
			<result name="success" >/jsp/demo/detailBankCardFeePage.jsp</result>
		</action>
		
		
		<!-- BankCardFee删除页面	-->
		<action name="delBankCardFeeAction" class="delBankCardFeeAction">
			<result name="success" type="chain"><param name="actionName">searchBankCardFeeAction</param> </result>
		</action>		

      
	</package>
</struts>