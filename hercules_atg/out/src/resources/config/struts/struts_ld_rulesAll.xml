<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_rulesAll"  extends="struts_common">
	  
	  	<!-- RulesAll列表信息 -->
        <action name="searchRulesAllAction" class="searchRulesAllAction">
			<result name="success" >/jsp/demo/searchRulesAllPage.jsp</result>
		</action>
		
        <!-- RulesAll新增页面-->
        <action name="addRulesAllPage" class="addRulesAllPage">
			<result name="success" >/jsp/demo/addRulesAllPage.jsp</result>
		</action>
		
		<!-- RulesAll新增动作	-->
        <action name="addRulesAllAction" class="addRulesAllAction"> 
			<result name="success" type="chain"><param name="actionName">searchRulesAllAction</param> </result>
		</action>
		
		<!-- RulesAll编辑页面 -->
		<action name="editRulesAllPage" class="editRulesAllPage">
			<result name="success" >/jsp/demo/editRulesAllPage.jsp</result>
		</action>

		
		<!-- RulesAll编辑动作-->
		<action name="editRulesAllAction" class="editRulesAllAction">
			<result name="success" type="chain"><param name="actionName">searchRulesAllAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailRulesAllPage" class="detailRulesAllPage">
			<result name="success" >/jsp/demo/detailRulesAllPage.jsp</result>
		</action>
		
		
		<!-- RulesAll删除页面	-->
		<action name="delRulesAllAction" class="delRulesAllAction">
			<result name="success" type="chain"><param name="actionName">searchRulesAllAction</param> </result>
		</action>		

      
	</package>
</struts>