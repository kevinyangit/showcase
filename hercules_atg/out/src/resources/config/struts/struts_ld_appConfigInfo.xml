<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_appConfigInfo"  extends="struts_common">
	  
	  	<!-- AppConfigInfo列表信息 -->
        <action name="searchAppConfigInfoAction" class="searchAppConfigInfoAction">
			<result name="success" >/jsp/demo/searchAppConfigInfoPage.jsp</result>
		</action>
		
        <!-- AppConfigInfo新增页面-->
        <action name="addAppConfigInfoPage" class="addAppConfigInfoPage">
			<result name="success" >/jsp/demo/addAppConfigInfoPage.jsp</result>
		</action>
		
		<!-- AppConfigInfo新增动作	-->
        <action name="addAppConfigInfoAction" class="addAppConfigInfoAction"> 
			<result name="success" type="chain"><param name="actionName">searchAppConfigInfoAction</param> </result>
		</action>
		
		<!-- AppConfigInfo编辑页面 -->
		<action name="editAppConfigInfoPage" class="editAppConfigInfoPage">
			<result name="success" >/jsp/demo/editAppConfigInfoPage.jsp</result>
		</action>

		
		<!-- AppConfigInfo编辑动作-->
		<action name="editAppConfigInfoAction" class="editAppConfigInfoAction">
			<result name="success" type="chain"><param name="actionName">searchAppConfigInfoAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailAppConfigInfoPage" class="detailAppConfigInfoPage">
			<result name="success" >/jsp/demo/detailAppConfigInfoPage.jsp</result>
		</action>
		
		
		<!-- AppConfigInfo删除页面	-->
		<action name="delAppConfigInfoAction" class="delAppConfigInfoAction">
			<result name="success" type="chain"><param name="actionName">searchAppConfigInfoAction</param> </result>
		</action>		

      
	</package>
</struts>