<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_orderRisk"  extends="struts_common">
	  
	  	<!-- OrderRisk列表信息 -->
        <action name="searchOrderRiskAction" class="searchOrderRiskAction">
			<result name="success" >/jsp/demo/searchOrderRiskPage.jsp</result>
		</action>
		
        <!-- OrderRisk新增页面-->
        <action name="addOrderRiskPage" class="addOrderRiskPage">
			<result name="success" >/jsp/demo/addOrderRiskPage.jsp</result>
		</action>
		
		<!-- OrderRisk新增动作	-->
        <action name="addOrderRiskAction" class="addOrderRiskAction"> 
			<result name="success" type="chain"><param name="actionName">searchOrderRiskAction</param> </result>
		</action>
		
		<!-- OrderRisk编辑页面 -->
		<action name="editOrderRiskPage" class="editOrderRiskPage">
			<result name="success" >/jsp/demo/editOrderRiskPage.jsp</result>
		</action>

		
		<!-- OrderRisk编辑动作-->
		<action name="editOrderRiskAction" class="editOrderRiskAction">
			<result name="success" type="chain"><param name="actionName">searchOrderRiskAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailOrderRiskPage" class="detailOrderRiskPage">
			<result name="success" >/jsp/demo/detailOrderRiskPage.jsp</result>
		</action>
		
		
		<!-- OrderRisk删除页面	-->
		<action name="delOrderRiskAction" class="delOrderRiskAction">
			<result name="success" type="chain"><param name="actionName">searchOrderRiskAction</param> </result>
		</action>		

      
	</package>
</struts>