<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_subMerSignSettleProfit"  extends="struts_common">
	  
	  	<!-- SubMerSignSettleProfit列表信息 -->
        <action name="searchSubMerSignSettleProfitAction" class="searchSubMerSignSettleProfitAction">
			<result name="success" >/jsp/demo/searchSubMerSignSettleProfitPage.jsp</result>
		</action>
		
        <!-- SubMerSignSettleProfit新增页面-->
        <action name="addSubMerSignSettleProfitPage" class="addSubMerSignSettleProfitPage">
			<result name="success" >/jsp/demo/addSubMerSignSettleProfitPage.jsp</result>
		</action>
		
		<!-- SubMerSignSettleProfit新增动作	-->
        <action name="addSubMerSignSettleProfitAction" class="addSubMerSignSettleProfitAction"> 
			<result name="success" type="chain"><param name="actionName">searchSubMerSignSettleProfitAction</param> </result>
		</action>
		
		<!-- SubMerSignSettleProfit编辑页面 -->
		<action name="editSubMerSignSettleProfitPage" class="editSubMerSignSettleProfitPage">
			<result name="success" >/jsp/demo/editSubMerSignSettleProfitPage.jsp</result>
		</action>

		
		<!-- SubMerSignSettleProfit编辑动作-->
		<action name="editSubMerSignSettleProfitAction" class="editSubMerSignSettleProfitAction">
			<result name="success" type="chain"><param name="actionName">searchSubMerSignSettleProfitAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailSubMerSignSettleProfitPage" class="detailSubMerSignSettleProfitPage">
			<result name="success" >/jsp/demo/detailSubMerSignSettleProfitPage.jsp</result>
		</action>
		
		
		<!-- SubMerSignSettleProfit删除页面	-->
		<action name="delSubMerSignSettleProfitAction" class="delSubMerSignSettleProfitAction">
			<result name="success" type="chain"><param name="actionName">searchSubMerSignSettleProfitAction</param> </result>
		</action>		

      
	</package>
</struts>