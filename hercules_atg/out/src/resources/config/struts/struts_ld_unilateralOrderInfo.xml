<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_unilateralOrderInfo"  extends="struts_common">
	  
	  	<!-- UnilateralOrderInfo列表信息 -->
        <action name="searchUnilateralOrderInfoAction" class="searchUnilateralOrderInfoAction">
			<result name="success" >/jsp/demo/searchUnilateralOrderInfoPage.jsp</result>
		</action>
		
        <!-- UnilateralOrderInfo新增页面-->
        <action name="addUnilateralOrderInfoPage" class="addUnilateralOrderInfoPage">
			<result name="success" >/jsp/demo/addUnilateralOrderInfoPage.jsp</result>
		</action>
		
		<!-- UnilateralOrderInfo新增动作	-->
        <action name="addUnilateralOrderInfoAction" class="addUnilateralOrderInfoAction"> 
			<result name="success" type="chain"><param name="actionName">searchUnilateralOrderInfoAction</param> </result>
		</action>
		
		<!-- UnilateralOrderInfo编辑页面 -->
		<action name="editUnilateralOrderInfoPage" class="editUnilateralOrderInfoPage">
			<result name="success" >/jsp/demo/editUnilateralOrderInfoPage.jsp</result>
		</action>

		
		<!-- UnilateralOrderInfo编辑动作-->
		<action name="editUnilateralOrderInfoAction" class="editUnilateralOrderInfoAction">
			<result name="success" type="chain"><param name="actionName">searchUnilateralOrderInfoAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailUnilateralOrderInfoPage" class="detailUnilateralOrderInfoPage">
			<result name="success" >/jsp/demo/detailUnilateralOrderInfoPage.jsp</result>
		</action>
		
		
		<!-- UnilateralOrderInfo删除页面	-->
		<action name="delUnilateralOrderInfoAction" class="delUnilateralOrderInfoAction">
			<result name="success" type="chain"><param name="actionName">searchUnilateralOrderInfoAction</param> </result>
		</action>		

      
	</package>
</struts>