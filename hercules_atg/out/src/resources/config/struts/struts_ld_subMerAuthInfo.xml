<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_subMerAuthInfo"  extends="struts_common">
	  
	  	<!-- SubMerAuthInfo列表信息 -->
        <action name="searchSubMerAuthInfoAction" class="searchSubMerAuthInfoAction">
			<result name="success" >/jsp/demo/searchSubMerAuthInfoPage.jsp</result>
		</action>
		
        <!-- SubMerAuthInfo新增页面-->
        <action name="addSubMerAuthInfoPage" class="addSubMerAuthInfoPage">
			<result name="success" >/jsp/demo/addSubMerAuthInfoPage.jsp</result>
		</action>
		
		<!-- SubMerAuthInfo新增动作	-->
        <action name="addSubMerAuthInfoAction" class="addSubMerAuthInfoAction"> 
			<result name="success" type="chain"><param name="actionName">searchSubMerAuthInfoAction</param> </result>
		</action>
		
		<!-- SubMerAuthInfo编辑页面 -->
		<action name="editSubMerAuthInfoPage" class="editSubMerAuthInfoPage">
			<result name="success" >/jsp/demo/editSubMerAuthInfoPage.jsp</result>
		</action>

		
		<!-- SubMerAuthInfo编辑动作-->
		<action name="editSubMerAuthInfoAction" class="editSubMerAuthInfoAction">
			<result name="success" type="chain"><param name="actionName">searchSubMerAuthInfoAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailSubMerAuthInfoPage" class="detailSubMerAuthInfoPage">
			<result name="success" >/jsp/demo/detailSubMerAuthInfoPage.jsp</result>
		</action>
		
		
		<!-- SubMerAuthInfo删除页面	-->
		<action name="delSubMerAuthInfoAction" class="delSubMerAuthInfoAction">
			<result name="success" type="chain"><param name="actionName">searchSubMerAuthInfoAction</param> </result>
		</action>		

      
	</package>
</struts>