<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_regionCode"  extends="struts_common">
	  
	  	<!-- RegionCode列表信息 -->
        <action name="searchRegionCodeAction" class="searchRegionCodeAction">
			<result name="success" >/jsp/demo/searchRegionCodePage.jsp</result>
		</action>
		
        <!-- RegionCode新增页面-->
        <action name="addRegionCodePage" class="addRegionCodePage">
			<result name="success" >/jsp/demo/addRegionCodePage.jsp</result>
		</action>
		
		<!-- RegionCode新增动作	-->
        <action name="addRegionCodeAction" class="addRegionCodeAction"> 
			<result name="success" type="chain"><param name="actionName">searchRegionCodeAction</param> </result>
		</action>
		
		<!-- RegionCode编辑页面 -->
		<action name="editRegionCodePage" class="editRegionCodePage">
			<result name="success" >/jsp/demo/editRegionCodePage.jsp</result>
		</action>

		
		<!-- RegionCode编辑动作-->
		<action name="editRegionCodeAction" class="editRegionCodeAction">
			<result name="success" type="chain"><param name="actionName">searchRegionCodeAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailRegionCodePage" class="detailRegionCodePage">
			<result name="success" >/jsp/demo/detailRegionCodePage.jsp</result>
		</action>
		
		
		<!-- RegionCode删除页面	-->
		<action name="delRegionCodeAction" class="delRegionCodeAction">
			<result name="success" type="chain"><param name="actionName">searchRegionCodeAction</param> </result>
		</action>		

      
	</package>
</struts>