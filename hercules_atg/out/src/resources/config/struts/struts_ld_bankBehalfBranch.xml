<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_bankBehalfBranch"  extends="struts_common">
	  
	  	<!-- BankBehalfBranch列表信息 -->
        <action name="searchBankBehalfBranchAction" class="searchBankBehalfBranchAction">
			<result name="success" >/jsp/demo/searchBankBehalfBranchPage.jsp</result>
		</action>
		
        <!-- BankBehalfBranch新增页面-->
        <action name="addBankBehalfBranchPage" class="addBankBehalfBranchPage">
			<result name="success" >/jsp/demo/addBankBehalfBranchPage.jsp</result>
		</action>
		
		<!-- BankBehalfBranch新增动作	-->
        <action name="addBankBehalfBranchAction" class="addBankBehalfBranchAction"> 
			<result name="success" type="chain"><param name="actionName">searchBankBehalfBranchAction</param> </result>
		</action>
		
		<!-- BankBehalfBranch编辑页面 -->
		<action name="editBankBehalfBranchPage" class="editBankBehalfBranchPage">
			<result name="success" >/jsp/demo/editBankBehalfBranchPage.jsp</result>
		</action>

		
		<!-- BankBehalfBranch编辑动作-->
		<action name="editBankBehalfBranchAction" class="editBankBehalfBranchAction">
			<result name="success" type="chain"><param name="actionName">searchBankBehalfBranchAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailBankBehalfBranchPage" class="detailBankBehalfBranchPage">
			<result name="success" >/jsp/demo/detailBankBehalfBranchPage.jsp</result>
		</action>
		
		
		<!-- BankBehalfBranch删除页面	-->
		<action name="delBankBehalfBranchAction" class="delBankBehalfBranchAction">
			<result name="success" type="chain"><param name="actionName">searchBankBehalfBranchAction</param> </result>
		</action>		

      
	</package>
</struts>