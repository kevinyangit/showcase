<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_sequence"  extends="struts_common">
	  
	  	<!-- Sequence列表信息 -->
        <action name="searchSequenceAction" class="searchSequenceAction">
			<result name="success" >/jsp/demo/searchSequencePage.jsp</result>
		</action>
		
        <!-- Sequence新增页面-->
        <action name="addSequencePage" class="addSequencePage">
			<result name="success" >/jsp/demo/addSequencePage.jsp</result>
		</action>
		
		<!-- Sequence新增动作	-->
        <action name="addSequenceAction" class="addSequenceAction"> 
			<result name="success" type="chain"><param name="actionName">searchSequenceAction</param> </result>
		</action>
		
		<!-- Sequence编辑页面 -->
		<action name="editSequencePage" class="editSequencePage">
			<result name="success" >/jsp/demo/editSequencePage.jsp</result>
		</action>

		
		<!-- Sequence编辑动作-->
		<action name="editSequenceAction" class="editSequenceAction">
			<result name="success" type="chain"><param name="actionName">searchSequenceAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailSequencePage" class="detailSequencePage">
			<result name="success" >/jsp/demo/detailSequencePage.jsp</result>
		</action>
		
		
		<!-- Sequence删除页面	-->
		<action name="delSequenceAction" class="delSequenceAction">
			<result name="success" type="chain"><param name="actionName">searchSequenceAction</param> </result>
		</action>		

      
	</package>
</struts>