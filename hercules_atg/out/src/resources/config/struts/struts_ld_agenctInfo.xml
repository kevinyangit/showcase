<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_agenctInfo"  extends="struts_common">
	  
	  	<!-- AgenctInfo列表信息 -->
        <action name="searchAgenctInfoAction" class="searchAgenctInfoAction">
			<result name="success" >/jsp/demo/searchAgenctInfoPage.jsp</result>
		</action>
		
        <!-- AgenctInfo新增页面-->
        <action name="addAgenctInfoPage" class="addAgenctInfoPage">
			<result name="success" >/jsp/demo/addAgenctInfoPage.jsp</result>
		</action>
		
		<!-- AgenctInfo新增动作	-->
        <action name="addAgenctInfoAction" class="addAgenctInfoAction"> 
			<result name="success" type="chain"><param name="actionName">searchAgenctInfoAction</param> </result>
		</action>
		
		<!-- AgenctInfo编辑页面 -->
		<action name="editAgenctInfoPage" class="editAgenctInfoPage">
			<result name="success" >/jsp/demo/editAgenctInfoPage.jsp</result>
		</action>

		
		<!-- AgenctInfo编辑动作-->
		<action name="editAgenctInfoAction" class="editAgenctInfoAction">
			<result name="success" type="chain"><param name="actionName">searchAgenctInfoAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailAgenctInfoPage" class="detailAgenctInfoPage">
			<result name="success" >/jsp/demo/detailAgenctInfoPage.jsp</result>
		</action>
		
		
		<!-- AgenctInfo删除页面	-->
		<action name="delAgenctInfoAction" class="delAgenctInfoAction">
			<result name="success" type="chain"><param name="actionName">searchAgenctInfoAction</param> </result>
		</action>		

      
	</package>
</struts>