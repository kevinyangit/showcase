<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_stopSettleStatictis"  extends="struts_common">
	  
	  	<!-- StopSettleStatictis列表信息 -->
        <action name="searchStopSettleStatictisAction" class="searchStopSettleStatictisAction">
			<result name="success" >/jsp/demo/searchStopSettleStatictisPage.jsp</result>
		</action>
		
        <!-- StopSettleStatictis新增页面-->
        <action name="addStopSettleStatictisPage" class="addStopSettleStatictisPage">
			<result name="success" >/jsp/demo/addStopSettleStatictisPage.jsp</result>
		</action>
		
		<!-- StopSettleStatictis新增动作	-->
        <action name="addStopSettleStatictisAction" class="addStopSettleStatictisAction"> 
			<result name="success" type="chain"><param name="actionName">searchStopSettleStatictisAction</param> </result>
		</action>
		
		<!-- StopSettleStatictis编辑页面 -->
		<action name="editStopSettleStatictisPage" class="editStopSettleStatictisPage">
			<result name="success" >/jsp/demo/editStopSettleStatictisPage.jsp</result>
		</action>

		
		<!-- StopSettleStatictis编辑动作-->
		<action name="editStopSettleStatictisAction" class="editStopSettleStatictisAction">
			<result name="success" type="chain"><param name="actionName">searchStopSettleStatictisAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailStopSettleStatictisPage" class="detailStopSettleStatictisPage">
			<result name="success" >/jsp/demo/detailStopSettleStatictisPage.jsp</result>
		</action>
		
		
		<!-- StopSettleStatictis删除页面	-->
		<action name="delStopSettleStatictisAction" class="delStopSettleStatictisAction">
			<result name="success" type="chain"><param name="actionName">searchStopSettleStatictisAction</param> </result>
		</action>		

      
	</package>
</struts>