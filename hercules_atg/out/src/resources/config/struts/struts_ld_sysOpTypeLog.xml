<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_sysOpTypeLog"  extends="struts_common">
	  
	  	<!-- SysOpTypeLog列表信息 -->
        <action name="searchSysOpTypeLogAction" class="searchSysOpTypeLogAction">
			<result name="success" >/jsp/demo/searchSysOpTypeLogPage.jsp</result>
		</action>
		
        <!-- SysOpTypeLog新增页面-->
        <action name="addSysOpTypeLogPage" class="addSysOpTypeLogPage">
			<result name="success" >/jsp/demo/addSysOpTypeLogPage.jsp</result>
		</action>
		
		<!-- SysOpTypeLog新增动作	-->
        <action name="addSysOpTypeLogAction" class="addSysOpTypeLogAction"> 
			<result name="success" type="chain"><param name="actionName">searchSysOpTypeLogAction</param> </result>
		</action>
		
		<!-- SysOpTypeLog编辑页面 -->
		<action name="editSysOpTypeLogPage" class="editSysOpTypeLogPage">
			<result name="success" >/jsp/demo/editSysOpTypeLogPage.jsp</result>
		</action>

		
		<!-- SysOpTypeLog编辑动作-->
		<action name="editSysOpTypeLogAction" class="editSysOpTypeLogAction">
			<result name="success" type="chain"><param name="actionName">searchSysOpTypeLogAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailSysOpTypeLogPage" class="detailSysOpTypeLogPage">
			<result name="success" >/jsp/demo/detailSysOpTypeLogPage.jsp</result>
		</action>
		
		
		<!-- SysOpTypeLog删除页面	-->
		<action name="delSysOpTypeLogAction" class="delSysOpTypeLogAction">
			<result name="success" type="chain"><param name="actionName">searchSysOpTypeLogAction</param> </result>
		</action>		

      
	</package>
</struts>