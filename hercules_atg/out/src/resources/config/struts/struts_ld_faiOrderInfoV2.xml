<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_faiOrderInfoV2"  extends="struts_common">
	  
	  	<!-- FaiOrderInfoV2列表信息 -->
        <action name="searchFaiOrderInfoV2Action" class="searchFaiOrderInfoV2Action">
			<result name="success" >/jsp/demo/searchFaiOrderInfoV2Page.jsp</result>
		</action>
		
        <!-- FaiOrderInfoV2新增页面-->
        <action name="addFaiOrderInfoV2Page" class="addFaiOrderInfoV2Page">
			<result name="success" >/jsp/demo/addFaiOrderInfoV2Page.jsp</result>
		</action>
		
		<!-- FaiOrderInfoV2新增动作	-->
        <action name="addFaiOrderInfoV2Action" class="addFaiOrderInfoV2Action"> 
			<result name="success" type="chain"><param name="actionName">searchFaiOrderInfoV2Action</param> </result>
		</action>
		
		<!-- FaiOrderInfoV2编辑页面 -->
		<action name="editFaiOrderInfoV2Page" class="editFaiOrderInfoV2Page">
			<result name="success" >/jsp/demo/editFaiOrderInfoV2Page.jsp</result>
		</action>

		
		<!-- FaiOrderInfoV2编辑动作-->
		<action name="editFaiOrderInfoV2Action" class="editFaiOrderInfoV2Action">
			<result name="success" type="chain"><param name="actionName">searchFaiOrderInfoV2Action</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailFaiOrderInfoV2Page" class="detailFaiOrderInfoV2Page">
			<result name="success" >/jsp/demo/detailFaiOrderInfoV2Page.jsp</result>
		</action>
		
		
		<!-- FaiOrderInfoV2删除页面	-->
		<action name="delFaiOrderInfoV2Action" class="delFaiOrderInfoV2Action">
			<result name="success" type="chain"><param name="actionName">searchFaiOrderInfoV2Action</param> </result>
		</action>		

      
	</package>
</struts>