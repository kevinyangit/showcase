<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_agentManager"  extends="struts_common">
	  
	  	<!-- AgentManager列表信息 -->
        <action name="searchAgentManagerAction" class="searchAgentManagerAction">
			<result name="success" >/jsp/demo/searchAgentManagerPage.jsp</result>
		</action>
		
        <!-- AgentManager新增页面-->
        <action name="addAgentManagerPage" class="addAgentManagerPage">
			<result name="success" >/jsp/demo/addAgentManagerPage.jsp</result>
		</action>
		
		<!-- AgentManager新增动作	-->
        <action name="addAgentManagerAction" class="addAgentManagerAction"> 
			<result name="success" type="chain"><param name="actionName">searchAgentManagerAction</param> </result>
		</action>
		
		<!-- AgentManager编辑页面 -->
		<action name="editAgentManagerPage" class="editAgentManagerPage">
			<result name="success" >/jsp/demo/editAgentManagerPage.jsp</result>
		</action>

		
		<!-- AgentManager编辑动作-->
		<action name="editAgentManagerAction" class="editAgentManagerAction">
			<result name="success" type="chain"><param name="actionName">searchAgentManagerAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailAgentManagerPage" class="detailAgentManagerPage">
			<result name="success" >/jsp/demo/detailAgentManagerPage.jsp</result>
		</action>
		
		
		<!-- AgentManager删除页面	-->
		<action name="delAgentManagerAction" class="delAgentManagerAction">
			<result name="success" type="chain"><param name="actionName">searchAgentManagerAction</param> </result>
		</action>		

      
	</package>
</struts>