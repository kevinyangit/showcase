<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_subMerCashoutBatch"  extends="struts_common">
	  
	  	<!-- SubMerCashoutBatch列表信息 -->
        <action name="searchSubMerCashoutBatchAction" class="searchSubMerCashoutBatchAction">
			<result name="success" >/jsp/demo/searchSubMerCashoutBatchPage.jsp</result>
		</action>
		
        <!-- SubMerCashoutBatch新增页面-->
        <action name="addSubMerCashoutBatchPage" class="addSubMerCashoutBatchPage">
			<result name="success" >/jsp/demo/addSubMerCashoutBatchPage.jsp</result>
		</action>
		
		<!-- SubMerCashoutBatch新增动作	-->
        <action name="addSubMerCashoutBatchAction" class="addSubMerCashoutBatchAction"> 
			<result name="success" type="chain"><param name="actionName">searchSubMerCashoutBatchAction</param> </result>
		</action>
		
		<!-- SubMerCashoutBatch编辑页面 -->
		<action name="editSubMerCashoutBatchPage" class="editSubMerCashoutBatchPage">
			<result name="success" >/jsp/demo/editSubMerCashoutBatchPage.jsp</result>
		</action>

		
		<!-- SubMerCashoutBatch编辑动作-->
		<action name="editSubMerCashoutBatchAction" class="editSubMerCashoutBatchAction">
			<result name="success" type="chain"><param name="actionName">searchSubMerCashoutBatchAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailSubMerCashoutBatchPage" class="detailSubMerCashoutBatchPage">
			<result name="success" >/jsp/demo/detailSubMerCashoutBatchPage.jsp</result>
		</action>
		
		
		<!-- SubMerCashoutBatch删除页面	-->
		<action name="delSubMerCashoutBatchAction" class="delSubMerCashoutBatchAction">
			<result name="success" type="chain"><param name="actionName">searchSubMerCashoutBatchAction</param> </result>
		</action>		

      
	</package>
</struts>