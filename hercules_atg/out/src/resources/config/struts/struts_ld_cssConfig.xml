<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_cssConfig"  extends="struts_common">
	  
	  	<!-- CssConfig列表信息 -->
        <action name="searchCssConfigAction" class="searchCssConfigAction">
			<result name="success" >/jsp/demo/searchCssConfigPage.jsp</result>
		</action>
		
        <!-- CssConfig新增页面-->
        <action name="addCssConfigPage" class="addCssConfigPage">
			<result name="success" >/jsp/demo/addCssConfigPage.jsp</result>
		</action>
		
		<!-- CssConfig新增动作	-->
        <action name="addCssConfigAction" class="addCssConfigAction"> 
			<result name="success" type="chain"><param name="actionName">searchCssConfigAction</param> </result>
		</action>
		
		<!-- CssConfig编辑页面 -->
		<action name="editCssConfigPage" class="editCssConfigPage">
			<result name="success" >/jsp/demo/editCssConfigPage.jsp</result>
		</action>

		
		<!-- CssConfig编辑动作-->
		<action name="editCssConfigAction" class="editCssConfigAction">
			<result name="success" type="chain"><param name="actionName">searchCssConfigAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailCssConfigPage" class="detailCssConfigPage">
			<result name="success" >/jsp/demo/detailCssConfigPage.jsp</result>
		</action>
		
		
		<!-- CssConfig删除页面	-->
		<action name="delCssConfigAction" class="delCssConfigAction">
			<result name="success" type="chain"><param name="actionName">searchCssConfigAction</param> </result>
		</action>		

      
	</package>
</struts>