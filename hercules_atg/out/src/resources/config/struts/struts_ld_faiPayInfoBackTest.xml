<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_faiPayInfoBackTest"  extends="struts_common">
	  
	  	<!-- FaiPayInfoBackTest列表信息 -->
        <action name="searchFaiPayInfoBackTestAction" class="searchFaiPayInfoBackTestAction">
			<result name="success" >/jsp/demo/searchFaiPayInfoBackTestPage.jsp</result>
		</action>
		
        <!-- FaiPayInfoBackTest新增页面-->
        <action name="addFaiPayInfoBackTestPage" class="addFaiPayInfoBackTestPage">
			<result name="success" >/jsp/demo/addFaiPayInfoBackTestPage.jsp</result>
		</action>
		
		<!-- FaiPayInfoBackTest新增动作	-->
        <action name="addFaiPayInfoBackTestAction" class="addFaiPayInfoBackTestAction"> 
			<result name="success" type="chain"><param name="actionName">searchFaiPayInfoBackTestAction</param> </result>
		</action>
		
		<!-- FaiPayInfoBackTest编辑页面 -->
		<action name="editFaiPayInfoBackTestPage" class="editFaiPayInfoBackTestPage">
			<result name="success" >/jsp/demo/editFaiPayInfoBackTestPage.jsp</result>
		</action>

		
		<!-- FaiPayInfoBackTest编辑动作-->
		<action name="editFaiPayInfoBackTestAction" class="editFaiPayInfoBackTestAction">
			<result name="success" type="chain"><param name="actionName">searchFaiPayInfoBackTestAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailFaiPayInfoBackTestPage" class="detailFaiPayInfoBackTestPage">
			<result name="success" >/jsp/demo/detailFaiPayInfoBackTestPage.jsp</result>
		</action>
		
		
		<!-- FaiPayInfoBackTest删除页面	-->
		<action name="delFaiPayInfoBackTestAction" class="delFaiPayInfoBackTestAction">
			<result name="success" type="chain"><param name="actionName">searchFaiPayInfoBackTestAction</param> </result>
		</action>		

      
	</package>
</struts>