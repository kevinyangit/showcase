<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_importRecord"  extends="struts_common">
	  
	  	<!-- ImportRecord列表信息 -->
        <action name="searchImportRecordAction" class="searchImportRecordAction">
			<result name="success" >/jsp/demo/searchImportRecordPage.jsp</result>
		</action>
		
        <!-- ImportRecord新增页面-->
        <action name="addImportRecordPage" class="addImportRecordPage">
			<result name="success" >/jsp/demo/addImportRecordPage.jsp</result>
		</action>
		
		<!-- ImportRecord新增动作	-->
        <action name="addImportRecordAction" class="addImportRecordAction"> 
			<result name="success" type="chain"><param name="actionName">searchImportRecordAction</param> </result>
		</action>
		
		<!-- ImportRecord编辑页面 -->
		<action name="editImportRecordPage" class="editImportRecordPage">
			<result name="success" >/jsp/demo/editImportRecordPage.jsp</result>
		</action>

		
		<!-- ImportRecord编辑动作-->
		<action name="editImportRecordAction" class="editImportRecordAction">
			<result name="success" type="chain"><param name="actionName">searchImportRecordAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailImportRecordPage" class="detailImportRecordPage">
			<result name="success" >/jsp/demo/detailImportRecordPage.jsp</result>
		</action>
		
		
		<!-- ImportRecord删除页面	-->
		<action name="delImportRecordAction" class="delImportRecordAction">
			<result name="success" type="chain"><param name="actionName">searchImportRecordAction</param> </result>
		</action>		

      
	</package>
</struts>