<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_susUni"  extends="struts_common">
	  
	  	<!-- SusUni列表信息 -->
        <action name="searchSusUniAction" class="searchSusUniAction">
			<result name="success" >/jsp/demo/searchSusUniPage.jsp</result>
		</action>
		
        <!-- SusUni新增页面-->
        <action name="addSusUniPage" class="addSusUniPage">
			<result name="success" >/jsp/demo/addSusUniPage.jsp</result>
		</action>
		
		<!-- SusUni新增动作	-->
        <action name="addSusUniAction" class="addSusUniAction"> 
			<result name="success" type="chain"><param name="actionName">searchSusUniAction</param> </result>
		</action>
		
		<!-- SusUni编辑页面 -->
		<action name="editSusUniPage" class="editSusUniPage">
			<result name="success" >/jsp/demo/editSusUniPage.jsp</result>
		</action>

		
		<!-- SusUni编辑动作-->
		<action name="editSusUniAction" class="editSusUniAction">
			<result name="success" type="chain"><param name="actionName">searchSusUniAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailSusUniPage" class="detailSusUniPage">
			<result name="success" >/jsp/demo/detailSusUniPage.jsp</result>
		</action>
		
		
		<!-- SusUni删除页面	-->
		<action name="delSusUniAction" class="delSusUniAction">
			<result name="success" type="chain"><param name="actionName">searchSusUniAction</param> </result>
		</action>		

      
	</package>
</struts>