<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_terminalManagement"  extends="struts_common">
	  
	  	<!-- TerminalManagement列表信息 -->
        <action name="searchTerminalManagementAction" class="searchTerminalManagementAction">
			<result name="success" >/jsp/demo/searchTerminalManagementPage.jsp</result>
		</action>
		
        <!-- TerminalManagement新增页面-->
        <action name="addTerminalManagementPage" class="addTerminalManagementPage">
			<result name="success" >/jsp/demo/addTerminalManagementPage.jsp</result>
		</action>
		
		<!-- TerminalManagement新增动作	-->
        <action name="addTerminalManagementAction" class="addTerminalManagementAction"> 
			<result name="success" type="chain"><param name="actionName">searchTerminalManagementAction</param> </result>
		</action>
		
		<!-- TerminalManagement编辑页面 -->
		<action name="editTerminalManagementPage" class="editTerminalManagementPage">
			<result name="success" >/jsp/demo/editTerminalManagementPage.jsp</result>
		</action>

		
		<!-- TerminalManagement编辑动作-->
		<action name="editTerminalManagementAction" class="editTerminalManagementAction">
			<result name="success" type="chain"><param name="actionName">searchTerminalManagementAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailTerminalManagementPage" class="detailTerminalManagementPage">
			<result name="success" >/jsp/demo/detailTerminalManagementPage.jsp</result>
		</action>
		
		
		<!-- TerminalManagement删除页面	-->
		<action name="delTerminalManagementAction" class="delTerminalManagementAction">
			<result name="success" type="chain"><param name="actionName">searchTerminalManagementAction</param> </result>
		</action>		

      
	</package>
</struts>