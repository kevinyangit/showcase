<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_merManager"  extends="struts_common">
	  
	  	<!-- MerManager列表信息 -->
        <action name="searchMerManagerAction" class="searchMerManagerAction">
			<result name="success" >/jsp/demo/searchMerManagerPage.jsp</result>
		</action>
		
        <!-- MerManager新增页面-->
        <action name="addMerManagerPage" class="addMerManagerPage">
			<result name="success" >/jsp/demo/addMerManagerPage.jsp</result>
		</action>
		
		<!-- MerManager新增动作	-->
        <action name="addMerManagerAction" class="addMerManagerAction"> 
			<result name="success" type="chain"><param name="actionName">searchMerManagerAction</param> </result>
		</action>
		
		<!-- MerManager编辑页面 -->
		<action name="editMerManagerPage" class="editMerManagerPage">
			<result name="success" >/jsp/demo/editMerManagerPage.jsp</result>
		</action>

		
		<!-- MerManager编辑动作-->
		<action name="editMerManagerAction" class="editMerManagerAction">
			<result name="success" type="chain"><param name="actionName">searchMerManagerAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailMerManagerPage" class="detailMerManagerPage">
			<result name="success" >/jsp/demo/detailMerManagerPage.jsp</result>
		</action>
		
		
		<!-- MerManager删除页面	-->
		<action name="delMerManagerAction" class="delMerManagerAction">
			<result name="success" type="chain"><param name="actionName">searchMerManagerAction</param> </result>
		</action>		

      
	</package>
</struts>