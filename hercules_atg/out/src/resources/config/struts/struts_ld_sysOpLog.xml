<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_sysOpLog"  extends="struts_common">
	  
	  	<!-- SysOpLog列表信息 -->
        <action name="searchSysOpLogAction" class="searchSysOpLogAction">
			<result name="success" >/jsp/demo/searchSysOpLogPage.jsp</result>
		</action>
		
        <!-- SysOpLog新增页面-->
        <action name="addSysOpLogPage" class="addSysOpLogPage">
			<result name="success" >/jsp/demo/addSysOpLogPage.jsp</result>
		</action>
		
		<!-- SysOpLog新增动作	-->
        <action name="addSysOpLogAction" class="addSysOpLogAction"> 
			<result name="success" type="chain"><param name="actionName">searchSysOpLogAction</param> </result>
		</action>
		
		<!-- SysOpLog编辑页面 -->
		<action name="editSysOpLogPage" class="editSysOpLogPage">
			<result name="success" >/jsp/demo/editSysOpLogPage.jsp</result>
		</action>

		
		<!-- SysOpLog编辑动作-->
		<action name="editSysOpLogAction" class="editSysOpLogAction">
			<result name="success" type="chain"><param name="actionName">searchSysOpLogAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailSysOpLogPage" class="detailSysOpLogPage">
			<result name="success" >/jsp/demo/detailSysOpLogPage.jsp</result>
		</action>
		
		
		<!-- SysOpLog删除页面	-->
		<action name="delSysOpLogAction" class="delSysOpLogAction">
			<result name="success" type="chain"><param name="actionName">searchSysOpLogAction</param> </result>
		</action>		

      
	</package>
</struts>