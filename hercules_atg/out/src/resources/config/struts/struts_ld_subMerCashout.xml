<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_subMerCashout"  extends="struts_common">
	  
	  	<!-- SubMerCashout列表信息 -->
        <action name="searchSubMerCashoutAction" class="searchSubMerCashoutAction">
			<result name="success" >/jsp/demo/searchSubMerCashoutPage.jsp</result>
		</action>
		
        <!-- SubMerCashout新增页面-->
        <action name="addSubMerCashoutPage" class="addSubMerCashoutPage">
			<result name="success" >/jsp/demo/addSubMerCashoutPage.jsp</result>
		</action>
		
		<!-- SubMerCashout新增动作	-->
        <action name="addSubMerCashoutAction" class="addSubMerCashoutAction"> 
			<result name="success" type="chain"><param name="actionName">searchSubMerCashoutAction</param> </result>
		</action>
		
		<!-- SubMerCashout编辑页面 -->
		<action name="editSubMerCashoutPage" class="editSubMerCashoutPage">
			<result name="success" >/jsp/demo/editSubMerCashoutPage.jsp</result>
		</action>

		
		<!-- SubMerCashout编辑动作-->
		<action name="editSubMerCashoutAction" class="editSubMerCashoutAction">
			<result name="success" type="chain"><param name="actionName">searchSubMerCashoutAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailSubMerCashoutPage" class="detailSubMerCashoutPage">
			<result name="success" >/jsp/demo/detailSubMerCashoutPage.jsp</result>
		</action>
		
		
		<!-- SubMerCashout删除页面	-->
		<action name="delSubMerCashoutAction" class="delSubMerCashoutAction">
			<result name="success" type="chain"><param name="actionName">searchSubMerCashoutAction</param> </result>
		</action>		

      
	</package>
</struts>