<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_orderInfo1"  extends="struts_common">
	  
	  	<!-- OrderInfo1列表信息 -->
        <action name="searchOrderInfo1Action" class="searchOrderInfo1Action">
			<result name="success" >/jsp/demo/searchOrderInfo1Page.jsp</result>
		</action>
		
        <!-- OrderInfo1新增页面-->
        <action name="addOrderInfo1Page" class="addOrderInfo1Page">
			<result name="success" >/jsp/demo/addOrderInfo1Page.jsp</result>
		</action>
		
		<!-- OrderInfo1新增动作	-->
        <action name="addOrderInfo1Action" class="addOrderInfo1Action"> 
			<result name="success" type="chain"><param name="actionName">searchOrderInfo1Action</param> </result>
		</action>
		
		<!-- OrderInfo1编辑页面 -->
		<action name="editOrderInfo1Page" class="editOrderInfo1Page">
			<result name="success" >/jsp/demo/editOrderInfo1Page.jsp</result>
		</action>

		
		<!-- OrderInfo1编辑动作-->
		<action name="editOrderInfo1Action" class="editOrderInfo1Action">
			<result name="success" type="chain"><param name="actionName">searchOrderInfo1Action</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailOrderInfo1Page" class="detailOrderInfo1Page">
			<result name="success" >/jsp/demo/detailOrderInfo1Page.jsp</result>
		</action>
		
		
		<!-- OrderInfo1删除页面	-->
		<action name="delOrderInfo1Action" class="delOrderInfo1Action">
			<result name="success" type="chain"><param name="actionName">searchOrderInfo1Action</param> </result>
		</action>		

      
	</package>
</struts>