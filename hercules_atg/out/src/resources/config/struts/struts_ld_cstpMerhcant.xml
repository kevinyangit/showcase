<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_cstpMerhcant"  extends="struts_common">
	  
	  	<!-- CstpMerhcant列表信息 -->
        <action name="searchCstpMerhcantAction" class="searchCstpMerhcantAction">
			<result name="success" >/jsp/demo/searchCstpMerhcantPage.jsp</result>
		</action>
		
        <!-- CstpMerhcant新增页面-->
        <action name="addCstpMerhcantPage" class="addCstpMerhcantPage">
			<result name="success" >/jsp/demo/addCstpMerhcantPage.jsp</result>
		</action>
		
		<!-- CstpMerhcant新增动作	-->
        <action name="addCstpMerhcantAction" class="addCstpMerhcantAction"> 
			<result name="success" type="chain"><param name="actionName">searchCstpMerhcantAction</param> </result>
		</action>
		
		<!-- CstpMerhcant编辑页面 -->
		<action name="editCstpMerhcantPage" class="editCstpMerhcantPage">
			<result name="success" >/jsp/demo/editCstpMerhcantPage.jsp</result>
		</action>

		
		<!-- CstpMerhcant编辑动作-->
		<action name="editCstpMerhcantAction" class="editCstpMerhcantAction">
			<result name="success" type="chain"><param name="actionName">searchCstpMerhcantAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailCstpMerhcantPage" class="detailCstpMerhcantPage">
			<result name="success" >/jsp/demo/detailCstpMerhcantPage.jsp</result>
		</action>
		
		
		<!-- CstpMerhcant删除页面	-->
		<action name="delCstpMerhcantAction" class="delCstpMerhcantAction">
			<result name="success" type="chain"><param name="actionName">searchCstpMerhcantAction</param> </result>
		</action>		

      
	</package>
</struts>