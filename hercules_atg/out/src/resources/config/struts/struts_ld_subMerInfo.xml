<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_subMerInfo"  extends="struts_common">
	  
	  	<!-- SubMerInfo列表信息 -->
        <action name="searchSubMerInfoAction" class="searchSubMerInfoAction">
			<result name="success" >/jsp/demo/searchSubMerInfoPage.jsp</result>
		</action>
		
        <!-- SubMerInfo新增页面-->
        <action name="addSubMerInfoPage" class="addSubMerInfoPage">
			<result name="success" >/jsp/demo/addSubMerInfoPage.jsp</result>
		</action>
		
		<!-- SubMerInfo新增动作	-->
        <action name="addSubMerInfoAction" class="addSubMerInfoAction"> 
			<result name="success" type="chain"><param name="actionName">searchSubMerInfoAction</param> </result>
		</action>
		
		<!-- SubMerInfo编辑页面 -->
		<action name="editSubMerInfoPage" class="editSubMerInfoPage">
			<result name="success" >/jsp/demo/editSubMerInfoPage.jsp</result>
		</action>

		
		<!-- SubMerInfo编辑动作-->
		<action name="editSubMerInfoAction" class="editSubMerInfoAction">
			<result name="success" type="chain"><param name="actionName">searchSubMerInfoAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailSubMerInfoPage" class="detailSubMerInfoPage">
			<result name="success" >/jsp/demo/detailSubMerInfoPage.jsp</result>
		</action>
		
		
		<!-- SubMerInfo删除页面	-->
		<action name="delSubMerInfoAction" class="delSubMerInfoAction">
			<result name="success" type="chain"><param name="actionName">searchSubMerInfoAction</param> </result>
		</action>		

      
	</package>
</struts>