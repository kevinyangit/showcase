<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_subMerInfoDeleted"  extends="struts_common">
	  
	  	<!-- SubMerInfoDeleted列表信息 -->
        <action name="searchSubMerInfoDeletedAction" class="searchSubMerInfoDeletedAction">
			<result name="success" >/jsp/demo/searchSubMerInfoDeletedPage.jsp</result>
		</action>
		
        <!-- SubMerInfoDeleted新增页面-->
        <action name="addSubMerInfoDeletedPage" class="addSubMerInfoDeletedPage">
			<result name="success" >/jsp/demo/addSubMerInfoDeletedPage.jsp</result>
		</action>
		
		<!-- SubMerInfoDeleted新增动作	-->
        <action name="addSubMerInfoDeletedAction" class="addSubMerInfoDeletedAction"> 
			<result name="success" type="chain"><param name="actionName">searchSubMerInfoDeletedAction</param> </result>
		</action>
		
		<!-- SubMerInfoDeleted编辑页面 -->
		<action name="editSubMerInfoDeletedPage" class="editSubMerInfoDeletedPage">
			<result name="success" >/jsp/demo/editSubMerInfoDeletedPage.jsp</result>
		</action>

		
		<!-- SubMerInfoDeleted编辑动作-->
		<action name="editSubMerInfoDeletedAction" class="editSubMerInfoDeletedAction">
			<result name="success" type="chain"><param name="actionName">searchSubMerInfoDeletedAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailSubMerInfoDeletedPage" class="detailSubMerInfoDeletedPage">
			<result name="success" >/jsp/demo/detailSubMerInfoDeletedPage.jsp</result>
		</action>
		
		
		<!-- SubMerInfoDeleted删除页面	-->
		<action name="delSubMerInfoDeletedAction" class="delSubMerInfoDeletedAction">
			<result name="success" type="chain"><param name="actionName">searchSubMerInfoDeletedAction</param> </result>
		</action>		

      
	</package>
</struts>