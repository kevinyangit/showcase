<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_v2CstpCsn"  extends="struts_common">
	  
	  	<!-- V2CstpCsn列表信息 -->
        <action name="searchV2CstpCsnAction" class="searchV2CstpCsnAction">
			<result name="success" >/jsp/demo/searchV2CstpCsnPage.jsp</result>
		</action>
		
        <!-- V2CstpCsn新增页面-->
        <action name="addV2CstpCsnPage" class="addV2CstpCsnPage">
			<result name="success" >/jsp/demo/addV2CstpCsnPage.jsp</result>
		</action>
		
		<!-- V2CstpCsn新增动作	-->
        <action name="addV2CstpCsnAction" class="addV2CstpCsnAction"> 
			<result name="success" type="chain"><param name="actionName">searchV2CstpCsnAction</param> </result>
		</action>
		
		<!-- V2CstpCsn编辑页面 -->
		<action name="editV2CstpCsnPage" class="editV2CstpCsnPage">
			<result name="success" >/jsp/demo/editV2CstpCsnPage.jsp</result>
		</action>

		
		<!-- V2CstpCsn编辑动作-->
		<action name="editV2CstpCsnAction" class="editV2CstpCsnAction">
			<result name="success" type="chain"><param name="actionName">searchV2CstpCsnAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailV2CstpCsnPage" class="detailV2CstpCsnPage">
			<result name="success" >/jsp/demo/detailV2CstpCsnPage.jsp</result>
		</action>
		
		
		<!-- V2CstpCsn删除页面	-->
		<action name="delV2CstpCsnAction" class="delV2CstpCsnAction">
			<result name="success" type="chain"><param name="actionName">searchV2CstpCsnAction</param> </result>
		</action>		

      
	</package>
</struts>