<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_regionCodeNew"  extends="struts_common">
	  
	  	<!-- RegionCodeNew列表信息 -->
        <action name="searchRegionCodeNewAction" class="searchRegionCodeNewAction">
			<result name="success" >/jsp/demo/searchRegionCodeNewPage.jsp</result>
		</action>
		
        <!-- RegionCodeNew新增页面-->
        <action name="addRegionCodeNewPage" class="addRegionCodeNewPage">
			<result name="success" >/jsp/demo/addRegionCodeNewPage.jsp</result>
		</action>
		
		<!-- RegionCodeNew新增动作	-->
        <action name="addRegionCodeNewAction" class="addRegionCodeNewAction"> 
			<result name="success" type="chain"><param name="actionName">searchRegionCodeNewAction</param> </result>
		</action>
		
		<!-- RegionCodeNew编辑页面 -->
		<action name="editRegionCodeNewPage" class="editRegionCodeNewPage">
			<result name="success" >/jsp/demo/editRegionCodeNewPage.jsp</result>
		</action>

		
		<!-- RegionCodeNew编辑动作-->
		<action name="editRegionCodeNewAction" class="editRegionCodeNewAction">
			<result name="success" type="chain"><param name="actionName">searchRegionCodeNewAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailRegionCodeNewPage" class="detailRegionCodeNewPage">
			<result name="success" >/jsp/demo/detailRegionCodeNewPage.jsp</result>
		</action>
		
		
		<!-- RegionCodeNew删除页面	-->
		<action name="delRegionCodeNewAction" class="delRegionCodeNewAction">
			<result name="success" type="chain"><param name="actionName">searchRegionCodeNewAction</param> </result>
		</action>		

      
	</package>
</struts>