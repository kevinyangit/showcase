<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_testCopy"  extends="struts_common">
	  
	  	<!-- TestCopy列表信息 -->
        <action name="searchTestCopyAction" class="searchTestCopyAction">
			<result name="success" >/jsp/demo/searchTestCopyPage.jsp</result>
		</action>
		
        <!-- TestCopy新增页面-->
        <action name="addTestCopyPage" class="addTestCopyPage">
			<result name="success" >/jsp/demo/addTestCopyPage.jsp</result>
		</action>
		
		<!-- TestCopy新增动作	-->
        <action name="addTestCopyAction" class="addTestCopyAction"> 
			<result name="success" type="chain"><param name="actionName">searchTestCopyAction</param> </result>
		</action>
		
		<!-- TestCopy编辑页面 -->
		<action name="editTestCopyPage" class="editTestCopyPage">
			<result name="success" >/jsp/demo/editTestCopyPage.jsp</result>
		</action>

		
		<!-- TestCopy编辑动作-->
		<action name="editTestCopyAction" class="editTestCopyAction">
			<result name="success" type="chain"><param name="actionName">searchTestCopyAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailTestCopyPage" class="detailTestCopyPage">
			<result name="success" >/jsp/demo/detailTestCopyPage.jsp</result>
		</action>
		
		
		<!-- TestCopy删除页面	-->
		<action name="delTestCopyAction" class="delTestCopyAction">
			<result name="success" type="chain"><param name="actionName">searchTestCopyAction</param> </result>
		</action>		

      
	</package>
</struts>