<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_tractInfo"  extends="struts_common">
	  
	  	<!-- TractInfo列表信息 -->
        <action name="searchTractInfoAction" class="searchTractInfoAction">
			<result name="success" >/jsp/demo/searchTractInfoPage.jsp</result>
		</action>
		
        <!-- TractInfo新增页面-->
        <action name="addTractInfoPage" class="addTractInfoPage">
			<result name="success" >/jsp/demo/addTractInfoPage.jsp</result>
		</action>
		
		<!-- TractInfo新增动作	-->
        <action name="addTractInfoAction" class="addTractInfoAction"> 
			<result name="success" type="chain"><param name="actionName">searchTractInfoAction</param> </result>
		</action>
		
		<!-- TractInfo编辑页面 -->
		<action name="editTractInfoPage" class="editTractInfoPage">
			<result name="success" >/jsp/demo/editTractInfoPage.jsp</result>
		</action>

		
		<!-- TractInfo编辑动作-->
		<action name="editTractInfoAction" class="editTractInfoAction">
			<result name="success" type="chain"><param name="actionName">searchTractInfoAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailTractInfoPage" class="detailTractInfoPage">
			<result name="success" >/jsp/demo/detailTractInfoPage.jsp</result>
		</action>
		
		
		<!-- TractInfo删除页面	-->
		<action name="delTractInfoAction" class="delTractInfoAction">
			<result name="success" type="chain"><param name="actionName">searchTractInfoAction</param> </result>
		</action>		

      
	</package>
</struts>