<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_terminalBatchConfig"  extends="struts_common">
	  
	  	<!-- TerminalBatchConfig列表信息 -->
        <action name="searchTerminalBatchConfigAction" class="searchTerminalBatchConfigAction">
			<result name="success" >/jsp/demo/searchTerminalBatchConfigPage.jsp</result>
		</action>
		
        <!-- TerminalBatchConfig新增页面-->
        <action name="addTerminalBatchConfigPage" class="addTerminalBatchConfigPage">
			<result name="success" >/jsp/demo/addTerminalBatchConfigPage.jsp</result>
		</action>
		
		<!-- TerminalBatchConfig新增动作	-->
        <action name="addTerminalBatchConfigAction" class="addTerminalBatchConfigAction"> 
			<result name="success" type="chain"><param name="actionName">searchTerminalBatchConfigAction</param> </result>
		</action>
		
		<!-- TerminalBatchConfig编辑页面 -->
		<action name="editTerminalBatchConfigPage" class="editTerminalBatchConfigPage">
			<result name="success" >/jsp/demo/editTerminalBatchConfigPage.jsp</result>
		</action>

		
		<!-- TerminalBatchConfig编辑动作-->
		<action name="editTerminalBatchConfigAction" class="editTerminalBatchConfigAction">
			<result name="success" type="chain"><param name="actionName">searchTerminalBatchConfigAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailTerminalBatchConfigPage" class="detailTerminalBatchConfigPage">
			<result name="success" >/jsp/demo/detailTerminalBatchConfigPage.jsp</result>
		</action>
		
		
		<!-- TerminalBatchConfig删除页面	-->
		<action name="delTerminalBatchConfigAction" class="delTerminalBatchConfigAction">
			<result name="success" type="chain"><param name="actionName">searchTerminalBatchConfigAction</param> </result>
		</action>		

      
	</package>
</struts>