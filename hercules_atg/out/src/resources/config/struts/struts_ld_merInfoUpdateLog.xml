<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_merInfoUpdateLog"  extends="struts_common">
	  
	  	<!-- MerInfoUpdateLog列表信息 -->
        <action name="searchMerInfoUpdateLogAction" class="searchMerInfoUpdateLogAction">
			<result name="success" >/jsp/demo/searchMerInfoUpdateLogPage.jsp</result>
		</action>
		
        <!-- MerInfoUpdateLog新增页面-->
        <action name="addMerInfoUpdateLogPage" class="addMerInfoUpdateLogPage">
			<result name="success" >/jsp/demo/addMerInfoUpdateLogPage.jsp</result>
		</action>
		
		<!-- MerInfoUpdateLog新增动作	-->
        <action name="addMerInfoUpdateLogAction" class="addMerInfoUpdateLogAction"> 
			<result name="success" type="chain"><param name="actionName">searchMerInfoUpdateLogAction</param> </result>
		</action>
		
		<!-- MerInfoUpdateLog编辑页面 -->
		<action name="editMerInfoUpdateLogPage" class="editMerInfoUpdateLogPage">
			<result name="success" >/jsp/demo/editMerInfoUpdateLogPage.jsp</result>
		</action>

		
		<!-- MerInfoUpdateLog编辑动作-->
		<action name="editMerInfoUpdateLogAction" class="editMerInfoUpdateLogAction">
			<result name="success" type="chain"><param name="actionName">searchMerInfoUpdateLogAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailMerInfoUpdateLogPage" class="detailMerInfoUpdateLogPage">
			<result name="success" >/jsp/demo/detailMerInfoUpdateLogPage.jsp</result>
		</action>
		
		
		<!-- MerInfoUpdateLog删除页面	-->
		<action name="delMerInfoUpdateLogAction" class="delMerInfoUpdateLogAction">
			<result name="success" type="chain"><param name="actionName">searchMerInfoUpdateLogAction</param> </result>
		</action>		

      
	</package>
</struts>