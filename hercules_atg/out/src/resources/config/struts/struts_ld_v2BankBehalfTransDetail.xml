<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_v2BankBehalfTransDetail"  extends="struts_common">
	  
	  	<!-- V2BankBehalfTransDetail列表信息 -->
        <action name="searchV2BankBehalfTransDetailAction" class="searchV2BankBehalfTransDetailAction">
			<result name="success" >/jsp/demo/searchV2BankBehalfTransDetailPage.jsp</result>
		</action>
		
        <!-- V2BankBehalfTransDetail新增页面-->
        <action name="addV2BankBehalfTransDetailPage" class="addV2BankBehalfTransDetailPage">
			<result name="success" >/jsp/demo/addV2BankBehalfTransDetailPage.jsp</result>
		</action>
		
		<!-- V2BankBehalfTransDetail新增动作	-->
        <action name="addV2BankBehalfTransDetailAction" class="addV2BankBehalfTransDetailAction"> 
			<result name="success" type="chain"><param name="actionName">searchV2BankBehalfTransDetailAction</param> </result>
		</action>
		
		<!-- V2BankBehalfTransDetail编辑页面 -->
		<action name="editV2BankBehalfTransDetailPage" class="editV2BankBehalfTransDetailPage">
			<result name="success" >/jsp/demo/editV2BankBehalfTransDetailPage.jsp</result>
		</action>

		
		<!-- V2BankBehalfTransDetail编辑动作-->
		<action name="editV2BankBehalfTransDetailAction" class="editV2BankBehalfTransDetailAction">
			<result name="success" type="chain"><param name="actionName">searchV2BankBehalfTransDetailAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailV2BankBehalfTransDetailPage" class="detailV2BankBehalfTransDetailPage">
			<result name="success" >/jsp/demo/detailV2BankBehalfTransDetailPage.jsp</result>
		</action>
		
		
		<!-- V2BankBehalfTransDetail删除页面	-->
		<action name="delV2BankBehalfTransDetailAction" class="delV2BankBehalfTransDetailAction">
			<result name="success" type="chain"><param name="actionName">searchV2BankBehalfTransDetailAction</param> </result>
		</action>		

      
	</package>
</struts>