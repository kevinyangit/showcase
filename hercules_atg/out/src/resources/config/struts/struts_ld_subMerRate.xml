<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_subMerRate"  extends="struts_common">
	  
	  	<!-- SubMerRate列表信息 -->
        <action name="searchSubMerRateAction" class="searchSubMerRateAction">
			<result name="success" >/jsp/demo/searchSubMerRatePage.jsp</result>
		</action>
		
        <!-- SubMerRate新增页面-->
        <action name="addSubMerRatePage" class="addSubMerRatePage">
			<result name="success" >/jsp/demo/addSubMerRatePage.jsp</result>
		</action>
		
		<!-- SubMerRate新增动作	-->
        <action name="addSubMerRateAction" class="addSubMerRateAction"> 
			<result name="success" type="chain"><param name="actionName">searchSubMerRateAction</param> </result>
		</action>
		
		<!-- SubMerRate编辑页面 -->
		<action name="editSubMerRatePage" class="editSubMerRatePage">
			<result name="success" >/jsp/demo/editSubMerRatePage.jsp</result>
		</action>

		
		<!-- SubMerRate编辑动作-->
		<action name="editSubMerRateAction" class="editSubMerRateAction">
			<result name="success" type="chain"><param name="actionName">searchSubMerRateAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailSubMerRatePage" class="detailSubMerRatePage">
			<result name="success" >/jsp/demo/detailSubMerRatePage.jsp</result>
		</action>
		
		
		<!-- SubMerRate删除页面	-->
		<action name="delSubMerRateAction" class="delSubMerRateAction">
			<result name="success" type="chain"><param name="actionName">searchSubMerRateAction</param> </result>
		</action>		

      
	</package>
</struts>