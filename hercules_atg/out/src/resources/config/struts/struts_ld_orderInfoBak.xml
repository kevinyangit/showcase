<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_orderInfoBak"  extends="struts_common">
	  
	  	<!-- OrderInfoBak列表信息 -->
        <action name="searchOrderInfoBakAction" class="searchOrderInfoBakAction">
			<result name="success" >/jsp/demo/searchOrderInfoBakPage.jsp</result>
		</action>
		
        <!-- OrderInfoBak新增页面-->
        <action name="addOrderInfoBakPage" class="addOrderInfoBakPage">
			<result name="success" >/jsp/demo/addOrderInfoBakPage.jsp</result>
		</action>
		
		<!-- OrderInfoBak新增动作	-->
        <action name="addOrderInfoBakAction" class="addOrderInfoBakAction"> 
			<result name="success" type="chain"><param name="actionName">searchOrderInfoBakAction</param> </result>
		</action>
		
		<!-- OrderInfoBak编辑页面 -->
		<action name="editOrderInfoBakPage" class="editOrderInfoBakPage">
			<result name="success" >/jsp/demo/editOrderInfoBakPage.jsp</result>
		</action>

		
		<!-- OrderInfoBak编辑动作-->
		<action name="editOrderInfoBakAction" class="editOrderInfoBakAction">
			<result name="success" type="chain"><param name="actionName">searchOrderInfoBakAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailOrderInfoBakPage" class="detailOrderInfoBakPage">
			<result name="success" >/jsp/demo/detailOrderInfoBakPage.jsp</result>
		</action>
		
		
		<!-- OrderInfoBak删除页面	-->
		<action name="delOrderInfoBakAction" class="delOrderInfoBakAction">
			<result name="success" type="chain"><param name="actionName">searchOrderInfoBakAction</param> </result>
		</action>		

      
	</package>
</struts>