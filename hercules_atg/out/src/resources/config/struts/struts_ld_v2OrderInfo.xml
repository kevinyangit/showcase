<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_v2OrderInfo"  extends="struts_common">
	  
	  	<!-- V2OrderInfo列表信息 -->
        <action name="searchV2OrderInfoAction" class="searchV2OrderInfoAction">
			<result name="success" >/jsp/demo/searchV2OrderInfoPage.jsp</result>
		</action>
		
        <!-- V2OrderInfo新增页面-->
        <action name="addV2OrderInfoPage" class="addV2OrderInfoPage">
			<result name="success" >/jsp/demo/addV2OrderInfoPage.jsp</result>
		</action>
		
		<!-- V2OrderInfo新增动作	-->
        <action name="addV2OrderInfoAction" class="addV2OrderInfoAction"> 
			<result name="success" type="chain"><param name="actionName">searchV2OrderInfoAction</param> </result>
		</action>
		
		<!-- V2OrderInfo编辑页面 -->
		<action name="editV2OrderInfoPage" class="editV2OrderInfoPage">
			<result name="success" >/jsp/demo/editV2OrderInfoPage.jsp</result>
		</action>

		
		<!-- V2OrderInfo编辑动作-->
		<action name="editV2OrderInfoAction" class="editV2OrderInfoAction">
			<result name="success" type="chain"><param name="actionName">searchV2OrderInfoAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailV2OrderInfoPage" class="detailV2OrderInfoPage">
			<result name="success" >/jsp/demo/detailV2OrderInfoPage.jsp</result>
		</action>
		
		
		<!-- V2OrderInfo删除页面	-->
		<action name="delV2OrderInfoAction" class="delV2OrderInfoAction">
			<result name="success" type="chain"><param name="actionName">searchV2OrderInfoAction</param> </result>
		</action>		

      
	</package>
</struts>