<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_bankBranch"  extends="struts_common">
	  
	  	<!-- BankBranch列表信息 -->
        <action name="searchBankBranchAction" class="searchBankBranchAction">
			<result name="success" >/jsp/demo/searchBankBranchPage.jsp</result>
		</action>
		
        <!-- BankBranch新增页面-->
        <action name="addBankBranchPage" class="addBankBranchPage">
			<result name="success" >/jsp/demo/addBankBranchPage.jsp</result>
		</action>
		
		<!-- BankBranch新增动作	-->
        <action name="addBankBranchAction" class="addBankBranchAction"> 
			<result name="success" type="chain"><param name="actionName">searchBankBranchAction</param> </result>
		</action>
		
		<!-- BankBranch编辑页面 -->
		<action name="editBankBranchPage" class="editBankBranchPage">
			<result name="success" >/jsp/demo/editBankBranchPage.jsp</result>
		</action>

		
		<!-- BankBranch编辑动作-->
		<action name="editBankBranchAction" class="editBankBranchAction">
			<result name="success" type="chain"><param name="actionName">searchBankBranchAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailBankBranchPage" class="detailBankBranchPage">
			<result name="success" >/jsp/demo/detailBankBranchPage.jsp</result>
		</action>
		
		
		<!-- BankBranch删除页面	-->
		<action name="delBankBranchAction" class="delBankBranchAction">
			<result name="success" type="chain"><param name="actionName">searchBankBranchAction</param> </result>
		</action>		

      
	</package>
</struts>