<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_agentModelCopy"  extends="struts_common">
	  
	  	<!-- AgentModelCopy列表信息 -->
        <action name="searchAgentModelCopyAction" class="searchAgentModelCopyAction">
			<result name="success" >/jsp/demo/searchAgentModelCopyPage.jsp</result>
		</action>
		
        <!-- AgentModelCopy新增页面-->
        <action name="addAgentModelCopyPage" class="addAgentModelCopyPage">
			<result name="success" >/jsp/demo/addAgentModelCopyPage.jsp</result>
		</action>
		
		<!-- AgentModelCopy新增动作	-->
        <action name="addAgentModelCopyAction" class="addAgentModelCopyAction"> 
			<result name="success" type="chain"><param name="actionName">searchAgentModelCopyAction</param> </result>
		</action>
		
		<!-- AgentModelCopy编辑页面 -->
		<action name="editAgentModelCopyPage" class="editAgentModelCopyPage">
			<result name="success" >/jsp/demo/editAgentModelCopyPage.jsp</result>
		</action>

		
		<!-- AgentModelCopy编辑动作-->
		<action name="editAgentModelCopyAction" class="editAgentModelCopyAction">
			<result name="success" type="chain"><param name="actionName">searchAgentModelCopyAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailAgentModelCopyPage" class="detailAgentModelCopyPage">
			<result name="success" >/jsp/demo/detailAgentModelCopyPage.jsp</result>
		</action>
		
		
		<!-- AgentModelCopy删除页面	-->
		<action name="delAgentModelCopyAction" class="delAgentModelCopyAction">
			<result name="success" type="chain"><param name="actionName">searchAgentModelCopyAction</param> </result>
		</action>		

      
	</package>
</struts>