<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />
	<package name="struts_ld_merTractTypeInfo"  extends="struts_common">
	  
	  	<!-- MerTractTypeInfo列表信息 -->
        <action name="searchMerTractTypeInfoAction" class="searchMerTractTypeInfoAction">
			<result name="success" >/jsp/demo/searchMerTractTypeInfoPage.jsp</result>
		</action>
		
        <!-- MerTractTypeInfo新增页面-->
        <action name="addMerTractTypeInfoPage" class="addMerTractTypeInfoPage">
			<result name="success" >/jsp/demo/addMerTractTypeInfoPage.jsp</result>
		</action>
		
		<!-- MerTractTypeInfo新增动作	-->
        <action name="addMerTractTypeInfoAction" class="addMerTractTypeInfoAction"> 
			<result name="success" type="chain"><param name="actionName">searchMerTractTypeInfoAction</param> </result>
		</action>
		
		<!-- MerTractTypeInfo编辑页面 -->
		<action name="editMerTractTypeInfoPage" class="editMerTractTypeInfoPage">
			<result name="success" >/jsp/demo/editMerTractTypeInfoPage.jsp</result>
		</action>

		
		<!-- MerTractTypeInfo编辑动作-->
		<action name="editMerTractTypeInfoAction" class="editMerTractTypeInfoAction">
			<result name="success" type="chain"><param name="actionName">searchMerTractTypeInfoAction</param> </result>
			<result name="input">/jsp/demo/error.jsp</result>
		</action>
		
		
		<!-- 详细画面动作-->
		<action name="detailMerTractTypeInfoPage" class="detailMerTractTypeInfoPage">
			<result name="success" >/jsp/demo/detailMerTractTypeInfoPage.jsp</result>
		</action>
		
		
		<!-- MerTractTypeInfo删除页面	-->
		<action name="delMerTractTypeInfoAction" class="delMerTractTypeInfoAction">
			<result name="success" type="chain"><param name="actionName">searchMerTractTypeInfoAction</param> </result>
		</action>		

      
	</package>
</struts>